function _GET_URL(pathObj,fileType) {
	var path = pathObj.directory+"\\"+pathObj[fileType];
	var subDir = "?";
	if(pathObj.subDir){
		if(fileType=='content'){
			subDir +=("subdir="+fileType+"\\"+pathObj.subDir);
		}else{
			subDir +=("subdir="+fileType);
		}
	}
	return path+subDir;
}